<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BenchBase">
    <defName>StudyDesk</defName>
    <label>Study Desk</label>
    <thingClass>Building_WorkTable</thingClass>
    <description>A desk to write and study.</description>
    <graphicData>
      <texPath>Things/Building/StudyDesk</texPath>
	  <shaderType>CutoutComplex</shaderType>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3.5,1.5)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>	  
    </stuffCategories>
    <costStuffCount>75</costStuffCount>
    <altitudeLayer>Building</altitudeLayer>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>2500</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <ResearchSpeedFactor>0.75</ResearchSpeedFactor>
    </statBases>
    <size>(3,1)</size>
    <passability>PassThroughOnly</passability>
    <designationCategory>Production</designationCategory>
    <pathCost>70</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <tickerType>Normal</tickerType>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
    <comps>
	  <li Class="CompProperties_Facility">
        <statOffsets>
          <WorkTableWorkSpeedFactor>1.0</WorkTableWorkSpeedFactor>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
        <maxDistance>11.0</maxDistance>
      </li>
    </comps>
    <designationHotKey></designationHotKey>
    <placeWorkers>
		<li>PlaceWorker_ShowFacilitiesConnections</li>
    </placeWorkers>
  </ThingDef>

  <!--From ItchyFlea's Practise Target-->
  <ThingDef ParentName="BuildingBase">
    <defName>Target</defName>
    <label>Target</label>
    <!--<thingClass>Building</thingClass>-->
    <thingClass>Building_WorkTable</thingClass>
    <uiIconPath>Things/Building/Target_MenuIcon</uiIconPath>
    <graphicData>
      <texPath>Things/Building/Target</texPath>
      <shaderType>CutoutComplex</shaderType>
      <graphicClass>Graphic_Multi</graphicClass>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <description>A target for your sharpshooters to hone their skills with.</description>
    <size>(1,1)</size>
    <useHitPoints>False</useHitPoints>
    <!--<costList>
      <Steel>35</Steel>
    </costList>-->
    <stuffCategories>
      <li>Woody</li>
    </stuffCategories>
    <costStuffCount>25</costStuffCount>
    <statBases>
      <WorkToBuild>250</WorkToBuild>
      <Flammability>0</Flammability>
    </statBases>
    <passability>PassThroughOnly</passability>
    <!--<castEdgeShadows>true</castEdgeShadows>-->
    <pathCost>60</pathCost>
    <fillPercent>0.8</fillPercent>
    <designationCategory>Security</designationCategory>
    <!--<staticSunShadowHeight>0.20</staticSunShadowHeight>-->
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-10)</interactionCellOffset>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
  </ThingDef>

  <ThingDef ParentName="BuildingBase">
    <defName>MrDummy</defName>
    <label>training dummy</label>
    <thingClass>Building_WorkTable</thingClass>
    <description>A straw dummy for training melee skill.</description>
    <uiIconPath>Things/Building/MrDummy_north</uiIconPath>
    <graphicData>
      <texPath>Things/Building/MrDummy</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <!--<costList>
      <Steel>15</Steel>
      <WoodLog>10</WoodLog>
    </costList>-->
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <costStuffCount>35</costStuffCount>
    <!--<constructEffect>ConstructWood</constructEffect>-->
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <!--<castEdgeShadows>true</castEdgeShadows>-->
    <fillPercent>1</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>200</WorkToBuild>
      <MaxHitPoints>350</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(1,1)</size>
    <designationCategory>Security</designationCategory>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <!--<surfaceType>Item</surfaceType>-->
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
  </ThingDef>

</Defs>